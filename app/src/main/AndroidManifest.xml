<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.immortalplayer.zeroloop">

    <application
        android:allowBackup="true"
        android:icon="@drawable/gameicon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.nonexistentware.igor.zeroloop.start"
            android:screenOrientation="portrait">
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.nonexistentware.igor.zeroloop.result"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.nonexistentware.igor.zeroloop.main"
            android:screenOrientation="portrait"></activity>
        <activity android:name="com.nonexistentware.igor.zeroloop.info"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>